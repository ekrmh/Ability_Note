<import name="card_box" src="../common/component/card_box/card_box.ux"></import>
<template>
    <card_box title-msg="{{disPlayContent.ability}}" menu-msg="{{disPlayContent.buttonLinkArray}}" refresh-click={{getnotes}}>
        <progress type="circular" show="{{isDataFetching}}" />
        <div class="contentBox" for="{{viewNotesData}}">
            <div class="upAddressBox" onclick={{itemClicked}}>
                <image src="{{upImgUrl}}"></image>
                <text class="upAddress">{{$item.title}}</text>
                <text class="createTime">{{$item.createTime}}</text>
            </div>
            <text class="address_title">{{$item.desc}}</text>
        </div>
    </card_box>
</template>

<style lang="sass">
    @import "./addressSelect.scss";
</style>

<script>
    import { URL } from "../Common/js/img_url.js";
    import fetch from '@system.fetch';
    import router from '@system.router';
    
    module.exports = {
        data: {
            notes: [],
            viewNotesData: [],
            viewMore: false,
            isDataFetching: "false",
            disPlayContent: {
                "ability": {
                    "name": "Notes",
                    // .png image to base64 image
                    "logoUrl": "data:image/png;base64,"
                },
                "items": [
                    {
                        "carImageUrl": {
                            "small": {
                                "url": "https://example.com/car_comfort_small@1x.png",
                                "widthPixels": 72,
                                "heightPixels": 72
                            },
                            "medium": {
                                "url": "https://example.com/car_comfort_small@2x.png",
                                "widthPixels": 172,
                                "heightPixels": 172
                            },
                            "large": {
                                "url": "https://example.com/car_comfort_small@3x.png",
                                "widthPixels": 280,
                                "heightPixels": 280
                            }
                        },
                        "productType": "Special car",
                        "rideTypeDisplayNameCn": "Comfortable",
                        "currencyCode": "CNY",
                        "estimatePrice": 12.01,
                        "time": 15,
                        "distance": 8.02,
                        "srcAddrDisplayName": "Nanjing South Railway Station",
                        "dstAddrDisplayName": "New interface pedestrian street",
                        "buttonLinkArray": [{
                            "buttonImageURL": "",
                            "buttonText": "Calling a car",
                            "webURL": "",
                            "deepLink": {
                                "url": "OneTravel://flash/action?param1=xxx&param2=yyy",
                                "appName": "DiDi",
                                "appPackage": "com.didi.chuxing",
                                "minVersion": "",
                                "minAndroidAPILevel": "23"
                            },
                            "quickApp": {
                                "url": "hap://app/com.example/home?biz=flash&action=call&slat=32.03766&slng=118.78706&tlng=118.7909&tlat=32.024117",
                                "minPlatformVersion": "",
                                "minVersion": ""
                            }
                        }],
                        "dynamic": 1.5
                    }
                ],
                "buttonLinkArray": [{
                    "buttonText": "View more"
                }]
            },
            upImgUrl: URL.upUrl,
            offImgUrl: URL.offUrl
        },
        getnotes: function () {
            var that = this;
            this.isDataFetching = "true"
            fetch.fetch({
                url: 'https://huaweinote-ce91a.firebaseio.com/notes.json',
                header: { auth: "AIzaSyBfxNOUWZM2lP8Ne4FhDdeD_zKPKuFXL48" },
                success: function (response) {
                    var notes = []
                    let data = JSON.parse(response.data)
                    for (var k in data) {
                        if (data.hasOwnProperty(k)) {
                            notes.push(data[k]);
                        }
                    }
                    that.notes = notes.reverse();
                    that.viewNotesData = that.viewMore ? that.notes : that.notes.slice(0, 3);
                    that.isDataFetching = "false"

                    console.log(notes);
                },
                fail: function (data, code) {
                    console.log("handling fail, code = " + code);
                    that.isDataFetching = "false"

                }
            })
        },
        onInit: function () {
            this.$on('menu-click', this.menuClick);
            this.$on('refresh-click', this.getnotes);
            /*
            setInterval(() => {
                this.getnotes();
            }, 10 * 1000);
            */
            this.getnotes();

        },
        itemClicked: function() {
            console.log("itemClicked");
        },
        menuClick: function (event) {
            switch (event.detail.idx) {
                case 0:
                    this.viewMore = !this.viewMore
                    this.disPlayContent.buttonLinkArray[0].buttonText = this.viewMore ? "View less" : "View more";
                    this.viewNotesData = this.viewMore ? this.notes : this.notes.slice(0, 3);
                    break;
                case 1:
                    break;
                default:
                    break;
            }
        }
    }
</script>